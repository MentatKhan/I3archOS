download-path "~/Audio/Podcasts/%n"
max-downloads 3
player "mpv"

browser "vimb %u >/dev/null 2>&1 &"

podlist-format "%4i [%-5p %%] %-12S %b"

bind-key j down
bind-key k up
bind-key l next articlelist
bind-key h prev articlelist
bind-key L next-feed articlelist
bind-key H prev-feed articlelist

bind-key G end
bind-key g home

auto-reload yes
reload-time 30

#urls-source "opml"
#opml-url "http://save.vs.totalpartykill.ca/grab-bag/osr.opml"
