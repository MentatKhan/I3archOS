#/bin/bash


if [[ "$1" == "" ]]; then
	echo "Usage $1 <file grep> <file name>"
	exit
fi

if [[ "$2" == "" ]]; then
	echo "Usage $1 <file grep> <file name>"
	exit
fi

PROTO=$( ls ~/bin/proto | grep -m 1 $1)
echo $PROTO
if [[ "$PROTO" == "" ]]; then
	echo "Could not find file with search $1"
	exit
fi

echo Grabbing $PROTO
echo Copying $PROTO to PWD
cp ~/bin/proto/$PROTO $2

